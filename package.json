{
    "name": "saastro",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "astro dev --host --port 3000",
        "build": "astro check && astro build",
        "preview": "astro preview",
        "start": "wrangler pages dev",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "lint": "biome lint . --write",
        "cleanup": "pnpm run --stream --parallel --color \"/cleanup:(?!deps)/\"",
        "cleanup:dev": "pnpm dlx rimraf .{astro,data,wrangler} dist",
        "cleanup:deps": "pnpm dlx rimraf pnpm-lock.yaml node_modules",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "typecheck": "tsc -b --noEmit",
        "test": "vitest run",
        "wrangler": "wrangler"
    },
    "dependencies": {
        "@astrojs/check": "^0.9.4",
        "@astrojs/mdx": "^3.1.9",
        "@astrojs/react": "^3.6.3",
        "@astrojs/rss": "^4.0.9",
        "@types/react-dom": "^18.3.1",
        "@types/react": "^18.3.12",
        "astro": "^4.16.16",
        "lodash.throttle": "^4.1.1",
        "node-html-parser": "^6.1.13",
        "react-dom": "^18.3.1",
        "react": "^18.3.1",
        "rehype-autolink-headings": "^7.1.0",
        "rehype-slug": "^6.0.0",
        "sharp": "^0.33.5",
        "std-env": "^3.8.0"
    },
    "devDependencies": {
        "@astrojs/sitemap": "^3.2.1",
        "@astrojs/tailwind": "^5.1.2",
        "@biomejs/biome": "^1.9.4",
        "@rollup/plugin-yaml": "^4.1.2",
        "@shikijs/transformers": "^1.24.0",
        "@tailwindcss/typography": "^0.5.15",
        "@types/lodash.throttle": "^4.1.9",
        "autoprefixer": "^10.4.20",
        "cssnano": "^7.0.6",
        "happy-dom": "^15.11.7",
        "hastscript": "^9.0.0",
        "lucide-astro": "^0.462.0",
        "npm-check-updates": "17.1.11",
        "postcss": "^8.4.49",
        "sass": "^1.81.0",
        "shiki": "^1.24.0",
        "tailwindcss-motion": "^0.4.3-beta",
        "tailwindcss": "^3.4.15",
        "typescript": "^5.7.2",
        "vite": "^6.0.1",
        "vitest": "^2.1.6",
        "wrangler": "^3.91.0"
    },
    "packageManager": "pnpm@9.14.4"
}
