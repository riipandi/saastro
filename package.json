{
    "name": "saastro",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "astro dev --host --port 3000",
        "build": "astro check && astro build",
        "preview": "astro preview",
        "start": "wrangler pages dev",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "lint": "biome lint . --write",
        "cleanup": "pnpm run --stream --parallel --color \"/cleanup:(?!deps)/\"",
        "cleanup:dev": "pnpm dlx rimraf .{astro,data,output,wrangler} dist",
        "cleanup:deps": "pnpm dlx rimraf pnpm-lock.yaml node_modules",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "typecheck": "tsc -b --noEmit",
        "wrangler": "wrangler"
    },
    "dependencies": {
        "@alpinejs/collapse": "^3.14.8",
        "@alpinejs/persist": "^3.14.8",
        "@astrojs/alpinejs": "^0.4.3",
        "@astrojs/check": "^0.9.4",
        "@astrojs/mdx": "^4.0.7",
        "@astrojs/rss": "^4.0.11",
        "@astrojs/sitemap": "^3.2.1",
        "alpinejs": "^3.14.8",
        "astro": "^5.1.10",
        "consola": "^3.4.0",
        "rehype-autolink-headings": "^7.1.0",
        "rehype-slug": "^6.0.0",
        "sharp": "^0.33.5",
        "std-env": "^3.8.0"
    },
    "devDependencies": {
        "@astrojs/tailwind": "^5.1.5",
        "@biomejs/biome": "^1.9.4",
        "@rollup/plugin-yaml": "^4.1.2",
        "@shikijs/transformers": "^1.24.2",
        "@tailwindcss/typography": "^0.5.16",
        "@types/alpinejs": "^3.13.11",
        "@types/alpinejs__collapse": "^3.13.4",
        "@types/alpinejs__persist": "^3.13.4",
        "astro-compress": "^2.3.6",
        "astro-rename": "^1.1.2",
        "autoprefixer": "^10.4.20",
        "cssnano": "^7.0.6",
        "lucide-astro": "^0.473.0",
        "npm-check-updates": "17.1.14",
        "pathe": "^2.0.2",
        "postcss": "^8.5.1",
        "shiki": "^1.24.2",
        "tailwindcss": "^3.4.17",
        "tailwindcss-motion": "1.0.1",
        "typescript": "^5.7.3",
        "vite": "^6.0.11",
        "wrangler": "^3.106.0"
    },
    "packageManager": "pnpm@9.15.4"
}
