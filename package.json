{
    "name": "saastro",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "astro dev --host --port 3000",
        "build": "astro check && astro build",
        "preview": "astro preview",
        "start": "wrangler pages dev",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "lint": "biome lint . --write",
        "cleanup": "pnpm run --stream --parallel --color \"/cleanup:(?!deps)/\"",
        "cleanup:dev": "pnpm dlx rimraf .{astro,data,output,wrangler} dist",
        "cleanup:deps": "pnpm dlx rimraf pnpm-lock.yaml node_modules",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "typecheck": "tsc -b --noEmit",
        "test": "vitest run",
        "wrangler": "wrangler"
    },
    "dependencies": {
        "@alpinejs/collapse": "^3.14.7",
        "@alpinejs/persist": "^3.14.7",
        "alpinejs": "^3.14.7",
        "astro": "^5.0.9",
        "rehype-autolink-headings": "^7.1.0",
        "rehype-slug": "^6.0.0",
        "sharp": "^0.33.5",
        "std-env": "^3.8.0"
    },
    "devDependencies": {
        "@astrojs/alpinejs": "^0.4.0",
        "@astrojs/check": "^0.9.4",
        "@astrojs/mdx": "^4.0.2",
        "@astrojs/rss": "^4.0.10",
        "@astrojs/sitemap": "^3.2.1",
        "@astrojs/tailwind": "^5.1.3",
        "@biomejs/biome": "^1.9.4",
        "@rollup/plugin-yaml": "^4.1.2",
        "@shikijs/transformers": "^1.24.2",
        "@tailwindcss/typography": "^0.5.15",
        "@types/alpinejs": "^3.13.11",
        "@types/alpinejs__collapse": "^3.13.4",
        "@types/alpinejs__persist": "^3.13.4",
        "astro-compress": "^2.3.5",
        "astro-rename": "^1.1.2",
        "autoprefixer": "^10.4.20",
        "cssnano": "^7.0.6",
        "happy-dom": "^15.11.7",
        "hastscript": "^9.0.0",
        "lucide-astro": "^0.468.0",
        "npm-check-updates": "17.1.11",
        "pathe": "^1.1.2",
        "postcss": "^8.4.49",
        "sass": "^1.83.0",
        "shiki": "^1.24.2",
        "tailwindcss": "^3.4.17",
        "tailwindcss-motion": "0.4.5-beta",
        "typescript": "^5.7.2",
        "vite": "^6.0.3",
        "vitest": "^2.1.8",
        "wrangler": "^3.97.0"
    },
    "packageManager": "pnpm@9.15.0"
}
